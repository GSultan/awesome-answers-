<h1>Sign Up</h1>
<%= @user.errors.full_messages.join(', ') %>

<%= form_for @user do |f| %>
<div class="">
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
</div>
<div class="">
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
</div>
<div class="">
  <%= f.label :email %>
  <%= f.email_field :email %>
</div>
<div class="">
  <%= f.label :password %>
  <%= f.password_field :password %>
</div>
<div class="">
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
</div>
<%= f.submit %>

<% end %>
